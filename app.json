{
  "expo": {
    "name": "Jobs Mobile",
    "description": "An awesome app that helps you find jobs near you",
    "slug": "jobs-mobile",
    "privacy": "public",
    "sdkVersion": "29.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
